[Unit]
Description=0G NFT Database Server
After=network.target

[Service]
Type=simple
User=root
WorkingDirectory=/root/0gnft/server
ExecStart=/usr/bin/node /root/0gnft/server/server.js
Restart=always
RestartSec=10
StandardOutput=journal
StandardError=journal

[Install]
WantedBy=multi-user.target











